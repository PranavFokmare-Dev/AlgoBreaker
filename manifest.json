{
  "name": "AlgoBreaker",
  "description": "Extension to block recomendtaions on youtube",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "history","storage", "activeTab", "scripting", "tabs","alarms"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "popup.html"
  }
}